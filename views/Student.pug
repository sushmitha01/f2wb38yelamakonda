extends layout

block content
  h1= title
  - var results=[{Name:"Shashi",Age:22,Address:"Suryapet"},{Name:"ram",Age:25,Address:"Hyderabad"},{Name:"Sunil",Age:32,Address:"khammam"}];
    

  div(class="container-fluid")
    div(class="row")
      each item,index  in results
        div(class="col-sm-2" onclick=`buy(${index})`) Name is #{item.Name} <br /> Age is #{item.Age} <br /> Address is #{item.Address}
  script.
    function buy(x){alert(`You purchased item ${x}`)}
